{
  "name": "fungi-react",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "env-cmd NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.4",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "jest": "^20.0.4",
    "nodemon": "^1.18.7",
    "react-app-env": "^1.2.3"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.0.6",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.0",
    "cloudinary": "^1.26.2",
    "cloudinary-core": "^2.11.4",
    "cloudinary-react": "^1.7.0",
    "concurrently": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "focus-trap-react": "^8.7.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "mongoose": "^5.13.2",
    "nodemon": "^2.0.9",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-file-base64": "^1.0.3",
    "react-leaflet": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "semantic-ui-react": "^2.0.3",
    "web-vitals": "^1.0.1"
